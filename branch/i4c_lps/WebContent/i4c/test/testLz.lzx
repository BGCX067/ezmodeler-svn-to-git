    <debug fontsize="12" oninit="Debug.showInternalProperties = true" />
    
    <script>
    	global["lz"]["Keys"] = LzKeys;
    </script>  
    
    <button text="invoke getUserObject">
        <handler name="onclick">
            if ($debug) Debug.write(lz.Keys);
        </handler>
    </button>
    
    <button y="200" text="invoke setUserObject">
        <handler name="onclick">
            if ($debug) Debug.write(LzKeys);
        </handler>
    </button>
    

</canvas>